{
	"name": "choctaw-nation/choctaw-news-plugin",
	"description": "a custom news plugin for the Choctaw Country site",
	"type": "project",
	"config": {
		"allow-plugins": {
			"composer/installers": true,
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	},
	"scripts": {
		"phpcs": "phpcs inc/ templates/ template-parts/ index.php",
		"phpcbf": "phpcbf inc/ templates/ template-parts/ index.php"
	},
	"require-dev": {
		"wp-coding-standards/wpcs": "*"
	}
}
